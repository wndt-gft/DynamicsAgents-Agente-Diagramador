# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=gft-bu-gcp
GOOGLE_CLOUD_LOCATION=us-east5
GOOGLE_GENAI_USE_VERTEXAI=TRUE

# Discovery Engine
DISCOVERY_PROJECT_ID=gft-bu-gcp
DISCOVERY_LOCATION=global
DISCOVERY_ENGINE_ID=cmdb-test-2_1759151360004

# Google AI API (para desenvolvimento local)
#GOOGLE_API_KEY=your-google-ai-api-key

# Ou use Application Default Credentials
# GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json

# LiteLLM Configuration

# Configuração do modelo para Gemini
MODEL_AGENT_ARCHITECT=gemini-2.5-pro    # modelo real a ser usado pelo agente diagramador
MODEL_AGENT_SEARCH=gemini-2.5-pro       # modelo real a ser usado pelo agente de busca das siglas

# Exemplo de configuração do modelo para Anthropic Claude (descomente se necessário)
# MODEL_AGENT_ARCHITECT=claude-opus-4-1@20250805                      # modelo real a ser usado pelo agente diagramador
# MODEL_AGENT_ARCHITECT_LIB=google.adk.models.anthropic_llm:Claude    # opcional, factory customizada para Claude
# MODEL_AGENT_SEARCH=claude-opus-4-1@20250805                         # modelo real a ser usado pelo agente de busca das siglas
# MODEL_AGENT_SEARCH_LIB=google.adk.models.anthropic_llm:Claude       # opcional, factory customizada para Claude

# Exemplo de configuração do modelo para OpenAI GPT-4 (descomente se necessário)
# necessario instalar o pacote litellm "pip install litellm"
#MODEL_AGENT_ARCHITECT=gpt-4o                                         # modelo real a ser usado pelo agente
#MODEL_AGENT_ARCHITECT_LIB=litellm:OpenAIChatModel                    # opcional, factory customizada para OpenAI GPT
#MODEL_AGENT_SEARCH=gpt-4o                                            # modelo real a ser usado pelo agente de busca das siglas
#MODEL_AGENT_SEARCH_LIB=litellm:OpenAIChatModel                       # opcional, factory customizada para OpenAI GPT

# BiZZdesign Integration (opcional)
BIZZDESIGN_WORKSPACE_URL=https://your-tenant.bizzdesign.com
BIZZDESIGN_API_TOKEN=your-api-token

# Application Settings
DEPLOY_ENV=development
LOG_LEVEL=INFO

# Acronym search fallback suggestions
SUGGEST_ACRONYMS=false
# Leave blank for unlimited suggestions or set a positive integer limit
SUGGEST_ACRONYMS_LIMIT=

# Dynamic logging toggle for user inputs and agent responses
# Accepted truthy values: 1,true,yes,on,y (case-insensitive)
# Accepted falsy values: 0,false,no,off,n
LOGGING_ENABLE=true

# Public GCS object exposure (default enabled to allow public URL when signing not possible)
MAKE_GCS_OBJECT_PUBLIC=1
ALLOW_UNSIGNED_GCS_URL=1
